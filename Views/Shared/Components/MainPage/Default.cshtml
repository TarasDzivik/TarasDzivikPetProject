@model IQueryable<VehicleItem>

@if (Model?.Any() == true)
{
    <section>
        <h2>Виберіть транспорт</h2>
        <ul class="small-image-list">
            @foreach (VehicleItem entity in Model)
            {
                <li>
                    <a asp-area="" asp-controller="Vehicle" asp-action="Index" asp-route-id="@entity.vehicleType">
                        <img src="~/images/VehicleImages/@entity.TitleImagePath" alt="image" class="left"/>
                    </a>
                    <h4>
                        @entity.Name
                    </h4>
                    <p>
                        @entity.Price
                    </p>
                </li>
            }
        </ul>
    </section>
}